    "dotenv": "^16.5.0",
    "embla-carousel-autoplay": "^8.1.3",
    "embla-carousel-react": "^8.2.0",
    "firebase": "^11.9.1",
    "framer-motion": "^11.5.0",
    "genkit": "^1.8.0",
    "lucide-react": "^0.475.0",
    "next": "15.3.3",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
